{
  "name": "laravel-bundler",
  "version": "2.9.0",
  "description": "Modern and fast asset building tool for Laravel framework with better defaults.",
  "exports": {
    ".": "./src/index.js",
    "./src/recipes/*": "./src/recipes/*",
    "./src/loaders/*": "./src/loaders/*",
    "./src/*": "./src/*"
  },
  "type": "module",
  "files": [
    "src",
    "bin",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurk91/laravel-bundler.git"
  },
  "keywords": [
    "laravel",
    "vue",
    "laravel-mix",
    "webpack",
    "bundle",
    "asset"
  ],
  "author": "ankurk91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankurk91/laravel-bundler/issues"
  },
  "homepage": "https://github.com/ankurk91/laravel-bundler",
  "scripts": {
    "demo:dev": "cd example && webpack --define-process-env-node-env=development --progress",
    "demo:watch": "cd example && webpack watch --define-process-env-node-env=development --progress",
    "demo:hot": "cd example && webpack serve --hot --progress --define-process-env-node-env=development",
    "demo:prod": "cd example && webpack --progress --define-process-env-node-env=production"
  },
  "bin": {
    "bundler": "bin/bundler.js"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "browserslist-to-esbuild": "^2.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^5.3.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "esbuild": "^0.24.0",
    "file-loader": "^6.2.0",
    "get-port": "^7.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "sass": "^1.80.5",
    "sass-loader": "^16.0.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.96.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-laravel-mix-manifest": "^3.1.2",
    "webpack-merge": "^6.0.1",
    "webpack-remove-empty-scripts": "^1.0.4",
    "yargs-parser": "^21.1.1"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "devDependencies": {
    "vue": "^3.5.12",
    "vue-loader": "^17.4.2"
  }
}

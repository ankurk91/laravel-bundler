{
  "name": "laravel-bundler",
  "version": "3.0.0",
  "description": "Modern and fast asset building tool for Laravel framework with better defaults.",
  "exports": {
    ".": "./src/index.js",
    "./src/recipes/*": "./src/recipes/*",
    "./src/loaders/*": "./src/loaders/*",
    "./src/*": "./src/*"
  },
  "type": "module",
  "files": [
    "src",
    "bin",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurk91/laravel-bundler.git"
  },
  "keywords": [
    "laravel",
    "vue",
    "laravel-mix",
    "webpack",
    "bundle",
    "asset"
  ],
  "author": "ankurk91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankurk91/laravel-bundler/issues"
  },
  "homepage": "https://github.com/ankurk91/laravel-bundler",
  "scripts": {
    "demo:dev": "cd example && webpack --config-node-env=development --progress",
    "demo:watch": "cd example && webpack watch --config-node-env=development --progress",
    "demo:hot": "cd example && webpack serve --hot --progress --config-node-env=development",
    "demo:prod": "cd example && webpack --progress --config-node-env=production"
  },
  "bin": {
    "bundler": "bin/bundler.js"
  },
  "dependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "browserslist-to-esbuild": "^2.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^5.4.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "esbuild": "^0.25.5",
    "file-loader": "^6.2.0",
    "get-port": "^7.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-laravel-mix-manifest": "^3.1.2",
    "webpack-merge": "^6.0.1",
    "webpack-remove-empty-scripts": "^1.1.1",
    "yargs-parser": "^22.0.0"
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "devDependencies": {
    "vue": "^3.5.16",
    "vue-loader": "^17.4.2"
  }
}

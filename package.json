{
  "name": "laravel-bundler",
  "version": "2.4.2",
  "description": "Modern and fast asset building tool for Laravel framework with better defaults.",
  "exports": {
    ".": "./src/index.js",
    "./src/recipes/*": "./src/recipes/*",
    "./src/loaders/*": "./src/loaders/*",
    "./src/*": "./src/*"
  },
  "type": "module",
  "files": [
    "src",
    "bin",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankurk91/laravel-bundler.git"
  },
  "keywords": [
    "laravel",
    "vue",
    "laravel-mix",
    "webpack",
    "bundle",
    "asset"
  ],
  "author": "ankurk91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankurk91/laravel-bundler/issues"
  },
  "homepage": "https://github.com/ankurk91/laravel-bundler",
  "scripts": {
    "demo:dev": "cd example && webpack --define-process-env-node-env=development --progress",
    "demo:watch": "cd example && webpack watch --define-process-env-node-env=development --progress",
    "demo:hot": "cd example && webpack serve --hot --progress --define-process-env-node-env=development",
    "demo:prod": "cd example && webpack --progress --define-process-env-node-env=production"
  },
  "bin": {
    "bundler": "bin/bundler.js"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "browserslist-to-esbuild": "^1.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^5.2.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "esbuild": "^0.19.2",
    "file-loader": "^6.2.0",
    "get-port": "^7.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "sass": "^1.63.5",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-laravel-mix-manifest": "^3.1.2",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^1.0.1",
    "yargs-parser": "^21.1.1"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "devDependencies": {
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1"
  }
}
